{
    "private": true,
    "name": "react-component",
    "version": "0.0.1",
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "dev:demo": "npx pnpm -r --filter demo run dev",
        "build:demo": "npx pnpm -r --filter demo run build",
        "lint": "npx pnpm -r --filter=\"*\" run lint",
        "rm-dep": "npx rimraf ./node_modules && npx pnpm -r --filter=\"*\" run rm-dep",
        "prettier": "npx prettier **/* --write --ignore-unknown"
    },
    "devDependencies": {
        "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
        "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
        "@types/node": "^20.1.1",
        "@types/react": "^18.2.9",
        "@types/react-dom": "^18.2.4",
        "@typescript-eslint/eslint-plugin": "^5.57.1",
        "@typescript-eslint/parser": "^5.57.1",
        "@vitejs/plugin-react-swc": "^3.0.0",
        "eslint": "^8.38.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.3.4",
        "less": "^4.1.3",
        "prettier": "^3.2.4",
        "rimraf": "^5.0.5",
        "rollup-plugin-node-polyfills": "^0.2.1",
        "typescript": "^5.0.2",
        "vite": "^4.3.2"
    },
    "dependencies": {
        "classnames": "^2.3.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.21.3"
    }
}
