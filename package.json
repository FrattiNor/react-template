{
    "name": "webpack-v5-demo",
    "version": "1.0.0",
    "description": "",
    "license": "",
    "scripts": {
        "start": "npm run dev",
        "dev": "npm run check-version && npm run check-proxy && cross-env NODE_ENV=development webpack-dev-server --config webpack/webpack.dev.js",
        "build": "npm run check-version && cross-env NODE_ENV=production webpack --config webpack/webpack.prod.js",
        "build-with-version": "node ./script/buildWithVersion.js",
        "analyzer": "cross-env NODE_ENV=production webpack --config webpack/webpack.analyzer.js",
        "rm-dep": "npx rimraf ./node_modules",
        "reinstall": "npm run rm-dep && pnpm install",
        "stylelint": "stylelint **/*.{less,css} --fix",
        "eslint": "eslint **/*.{js,jsx,ts,tsx} --fix --quiet",
        "prettier": "prettier **/* --write --ignore-unknown",
        "check-version": "node ./script/checkGitBranch.js",
        "check-proxy": "node ./script/checkProxy.js",
        "browserslist": "browserslist"
    },
    "devDependencies": {
        "@babel/core": "^7.21.8",
        "@babel/plugin-transform-runtime": "^7.21.4",
        "@babel/preset-env": "^7.21.5",
        "@babel/preset-react": "^7.18.6",
        "@babel/preset-typescript": "^7.21.5",
        "@babel/runtime-corejs3": "^7.21.5",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
        "@soda/friendly-errors-webpack-plugin": "^1.8.1",
        "@types/node": "^17.0.45",
        "@types/react": "^18.2.5",
        "@types/react-dom": "^18.2.3",
        "@types/react-redux": "^7.1.25",
        "@types/react-router-dom": "^5.3.3",
        "@umijs/fabric": "^3.0.0",
        "autoprefixer": "^10.4.14",
        "babel-loader": "^8.3.0",
        "babel-plugin-import": "^1.13.6",
        "browserslist": "^4.21.5",
        "copy-webpack-plugin": "^11.0.0",
        "cross-env": "^7.0.3",
        "css-loader": "^6.7.3",
        "css-minimizer-webpack-plugin": "^3.4.1",
        "html-webpack-plugin": "^5.5.1",
        "less": "^4.1.3",
        "less-loader": "^10.2.0",
        "mini-css-extract-plugin": "^2.7.5",
        "node-polyfill-webpack-plugin": "^2.0.1",
        "portfinder": "^1.0.32",
        "postcss": "^8.4.23",
        "postcss-loader": "^6.2.1",
        "react-refresh": "^0.14.0",
        "rimraf": "^3.0.2",
        "style-loader": "^3.3.2",
        "typescript": "^4.9.5",
        "webpack": "^5.82.0",
        "webpack-bundle-analyzer": "^4.8.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^4.13.3",
        "webpack-merge": "^5.8.0",
        "webpackbar": "^5.0.2"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^1.9.5",
        "antd": "4.24.8",
        "classnames": "^2.3.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "^8.0.5",
        "react-router-dom": "^6.11.1",
        "resize-observer-polyfill": "^1.5.1"
    },
    "sideEffects": [
        "**/*.css",
        "**/*.less"
    ]
}
