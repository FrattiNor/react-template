{
    "name": "react-template",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "bin": "./dist/index.js",
    "scripts": {
        "dev": "cross-env NODE_ENV=development node --loader ts-node/esm ./src/index.ts",
        "dev-watch": "cross-env NODE_ENV=development nodemon --config nodemon.json ./src/index.ts",
        "dev-brk": "cross-env NODE_ENV=development node --inspect-brk=0.0.0.0:9229 --loader ts-node/esm ./src/index.ts",
        "build": "npx tsc && npx ncc build src2/index.js -o dist && npx rimraf src2 && node ./script/copy.cjs"
    },
    "devDependencies": {
        "@types/copyfiles": "^2.4.1",
        "@types/inquirer": "^9.0.4",
        "@types/node": "^20.8.3",
        "@typescript-eslint/eslint-plugin": "^6.7.4",
        "@typescript-eslint/parser": "^6.7.4",
        "@vercel/ncc": "^0.38.0",
        "copyfiles": "^2.4.1",
        "cross-env": "^7.0.3",
        "eslint": "^7.32.0",
        "nodemon": "^3.0.1",
        "prettier": "^3.0.3",
        "rimraf": "^5.0.5",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2"
    },
    "dependencies": {
        "axios": "^1.6.0",
        "chalk": "^5.3.0",
        "commander": "^11.0.0",
        "dayjs": "^1.11.10",
        "dotenv": "^16.3.1",
        "form-data": "^4.0.0",
        "got": "^13.0.0",
        "hpagent": "^1.2.0",
        "inquirer": "^9.2.11"
    }
}
